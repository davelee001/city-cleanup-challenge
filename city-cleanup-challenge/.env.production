# Frontend Environment Variables - Production
REACT_APP_API_URL=${REACT_APP_API_URL}
REACT_APP_API_VERSION=v1
REACT_APP_ENV=production

# Map Configuration
REACT_APP_MAPS_API_KEY=${REACT_APP_MAPS_API_KEY}
REACT_APP_MAP_CENTER_LAT=40.7128
REACT_APP_MAP_CENTER_LNG=-74.0060
REACT_APP_MAP_ZOOM=12

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Analytics
REACT_APP_GA_TRACKING_ID=${REACT_APP_GA_TRACKING_ID}
REACT_APP_MIXPANEL_TOKEN=${REACT_APP_MIXPANEL_TOKEN}

# Social Login
REACT_APP_GOOGLE_CLIENT_ID=${REACT_APP_GOOGLE_CLIENT_ID}
REACT_APP_FACEBOOK_APP_ID=${REACT_APP_FACEBOOK_APP_ID}

# Other Configuration
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr
REACT_APP_DEFAULT_LANGUAGE=en

# Expo Configuration
EXPO_PUBLIC_API_URL=${EXPO_PUBLIC_API_URL}
